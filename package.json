{
  "name": "pyrrha-webclient",
  "version": "1.0.0",
  "description": "Webclient for Pyrrha",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sync:abis": "npx rimraf ./src/abis && cp -R ./pyrrha-abi ./src",
    "start": "npm run sync:abis && BROWSER=none npx react-app-rewired start",
    "build": "npx react-app-rewired build",
    "build:docker": "docker-compose build --no-cache",
    "start:docker": "docker-compose up -d --remove-orphans",
    "stop:docker": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandoraboxchain/pyrrha-webclient.git"
  },
  "bugs": {
    "url": "https://github.com/pandoraboxchain/pyrrha-webclient/issues"
  },
  "homepage": ".",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "ipfs-api": "^18.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^0.78.3",
    "web3": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "css-loader": "^0.28.10",
    "node-sass-chokidar": "^1.1.0",
    "react-app-rewired": "^1.5.0",
    "react-scripts": "^2.0.0-next.47d2d941",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "solhint": "^1.1.10",
    "style-loader": "^0.20.2"
  },
  "author": "Kostiantyn Smyrnov <kostysh@gmail.com>",
  "license": "MIT",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
